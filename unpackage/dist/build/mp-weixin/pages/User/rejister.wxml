tom"><button class="{{['but',(imgSrc=='../../static/user/Cicon1.png')?'active':'']}}" type="primary" loading="{{loading}}" disabled="{{imgSrc=='../../static/user/Cicon.png'}}" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">注册</button></view><lotus-address vue-id="1" lotusAddressData="{{lotusAddressData}}" data-event-opts="{{[['^choseVal',[['choseValue']]]]}}" bind:choseVal="__e" bind:__l="__l"></lotus-address></view>