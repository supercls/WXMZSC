.wrap-user {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
      -ms-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
      -ms-flex-pack: center;
          justify-content: center;
  padding: 0 60rpx 0 60rpx;
}
.wrap-user .user-logo {
    padding: 74rpx 0 0 0;
}
.wrap-user .user-logo text {
      font-size: 60rpx;
      color: #FF70B5;
      font-weight: Medium;
}
.wrap-user .user-center {
    margin-top: 74rpx;
    width: 100%;
}
.wrap-user .user-center .item {
      display: -webkit-box;
      display: -webkit-flex;
      display: -ms-flexbox;
      display: flex;
      border-bottom: 1rpx solid #E5E5E5;
      -webkit-box-align: center;
      -webkit-align-items: center;
          -ms-flex-align: center;
              align-items: center;
      padding: 30rpx 0;
}
.wrap-user .user-center .item .timeSpan {
        color: #FF70B5;
        font-size: 28rpx;
}
.wrap-user .user-center .item .active {
        color: #ccc;
}
.wrap-user .user-center .item .uni-img {
        width: 26rpx;
        height: 28rpx;
        display: inline-block;
        margin-right: 28rpx;
}
.wrap-user .user-center .item .placeholder {
        color: #999;
}
.wrap-user .user-center .item .uni-input {
        font-size: 32rpx;
        color: #333333;
        -webkit-box-flex: 1;
        -webkit-flex: 1;
            -ms-flex: 1;
                flex: 1;
}
.wrap-user .user-bottom .but {
    margin-top: 118rpx;
    width: 630rpx;
    height: 88rpx;
    background-color: #FF70B5;
    color: #fff;
    font-size: 36rpx;
    border-radius: 44rpx;
}
.wrap-user .user-bottom .active {
    background-color: #FF70B5;
}
.wrap-user .user-bottom .bot {
    margin-top: 40rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
}
.wrap-user .user-bottom .bot text {
      -webkit-box-flex: 1;
      -webkit-flex: 1;
          -ms-flex: 1;
              flex: 1;
      text-align: center;
      font-size: 28rpx;
      color: #FF70B5;
}
.wrap-user .user-tc {
    width: 100%;
    margin-top: 60rpx;
}
.wrap-user .user-tc .user-tc-v {
      display: inline-block;
}
.wrap-user .user-tc image {
      width: 30rpx;
      height: 30rpx;
      margin-right: 20rpx;
      vertical-align: -3.5rpx;
}
.wrap-user .user-tc text {
      font-size: 28rpx;
      color: #999999;
}
.wrap-user .user-tc .user-tc-t1 {
      color: #FF70B5;
}

