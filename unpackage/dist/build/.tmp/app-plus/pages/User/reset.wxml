<view class="wrap-user"><view class="user-center"><view class="item"><image class="uni-img" src="../../static/user/user_1.png" mode></image><input class="uni-input" type="number" placeholder-class="placeholder" placeholder="请输入手机号" maxlength="11" data-event-opts="{{[['input',[['__set_model',['$0','userCode','$event',[]],['userObj']]]]]}}" value="{{userObj.userCode}}" bindinput="__e"/></view><view class="item"><image class="uni-img" src="../../static/user/user_3.png" mode></image><input class="uni-input" placeholder-class="placeholder" placeholder="请输入短信验证码" type="number" data-event-opts="{{[['input',[['__set_model',['$0','verCode','$event',[]],['userObj']]]]]}}" value="{{userObj.verCode}}" bindinput="__e"/><text class="{{['timeSpan',(isSend)?'active':'']}}" disabled="{{isSend}}" data-event-opts="{{[['tap',[['sendCode',['$event']]]]]}}" bindtap="__e">{{times}}</text></view><view class="item"><image class="uni-img" src="../../static/user/user_2.png" mode></image><input class="uni-input" placeholder-class="placeholder" placeholder="请输入新登录密码" password data-event-opts="{{[['input',[['__set_model',['$0','passWord','$event',[]],['userObj']]]]]}}" value="{{userObj.passWord}}" bindinput="__e"/></view></view><view class="user-bottom"><button class="but" type="primary" loading="{{loading}}" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">重置密码</button></view></view>