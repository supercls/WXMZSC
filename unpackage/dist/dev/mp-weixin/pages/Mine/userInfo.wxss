.wrap-mine {
  width: 100%;
}
.wrap-mine .content {
    background: #fff;
}
.wrap-mine .mine-item {
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    height: 130rpx;
    margin: 0 30rpx;
    line-height: 130rpx;
    background: #fff;
    -webkit-box-align: center;
    -webkit-align-items: center;
        -ms-flex-align: center;
            align-items: center;
    border-bottom: 1rpx solid #E5E5E5;
}
.wrap-mine .mine-item .item-img {
      width: 98rpx;
      height: 98rpx;
      border-radius: 50%;
      -webkit-box-pack: center;
      -webkit-justify-content: center;
          -ms-flex-pack: center;
              justify-content: center;
}
.wrap-mine .mine-item navigator {
      -webkit-box-flex: 1;
      -webkit-flex: 1;
          -ms-flex: 1;
              flex: 1;
      text-align: right;
}
.wrap-mine .mine-item .item-text {
      -webkit-box-flex: 1;
      -webkit-flex: 1;
          -ms-flex: 1;
              flex: 1;
      text-align: right;
      padding-right: 20rpx;
      font-size: 28rpx;
      color: #999999;
}
.wrap-mine .mine-item .item-icon {
      width: 14rpx;
      height: 26rpx;
}
.wrap-mine .mine-item .item-left {
      color: #333;
      font-size: 32rpx;
      font-weight: Regular;
}
.wrap-mine .empty {
    width: 100%;
    height: 10rpx;
    background: #F2F2F2;
}

